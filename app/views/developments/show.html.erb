<%- model_class = Development -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:operation_id) %>:</strong></dt>
  <dd><%= @development.operation_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:maquina) %>:</strong></dt>
  <dd><%= @development.maquina %></dd>
  <dt><strong><%= model_class.human_attribute_name(:calibre_aguja) %>:</strong></dt>
  <dd><%= @development.calibre_aguja %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ppp) %>:</strong></dt>
  <dd><%= @development.ppp %></dd>
  <dt><strong><%= model_class.human_attribute_name(:margen_costura) %>:</strong></dt>
  <dd><%= @development.margen_costura %></dd>
  <dt><strong><%= model_class.human_attribute_name(:guia_accesorios) %>:</strong></dt>
  <dd><%= @development.guia_accesorios %></dd>
  <dt><strong><%= model_class.human_attribute_name(:observaciones) %>:</strong></dt>
  <dd><%= @development.observaciones %></dd>
  <dt><strong><%= model_class.human_attribute_name(:info_id) %>:</strong></dt>
  <dd><%= @development.info_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
  development_path(development), :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_development_path(development), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              development_path(@development),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
