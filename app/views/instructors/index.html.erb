modulo_instructor_path(@modulo)<%- model_class = Instructor -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:tipo_doc_id) %></th>
      <th><%= model_class.human_attribute_name(:numero_doc) %></th>
      <th><%= model_class.human_attribute_name(:nombres) %></th>
      <th><%= model_class.human_attribute_name(:apellidos) %></th>
      <th><%= model_class.human_attribute_name(:correo) %></th>
      <th><%= model_class.human_attribute_name(:telefono) %></th>
      <th><%= model_class.human_attribute_name(:modulo_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @instructors.each do |instructor| %>
      <tr>
        <td><%= link_to instructor.id, instructor_path(instructor) %></td>
        <td><%= instructor.tipo_doc_id %></td>
        <td><%= instructor.numero_doc %></td>
        <td><%= instructor.nombres %></td>
        <td><%= instructor.apellidos %></td>
        <td><%= instructor.correo %></td>
        <td><%= instructor.telefono %></td>
        <td><%= instructor.modulo_id %></td>
        <td><%=l instructor.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
    edit_modulo_instructor_path(@modulo,instructor), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
    modulo_instructor_path(@modulo, instructor),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
  new_modulo_instructor_path(@modulo),
            :class => 'btn btn-primary' %>
<%= link_to 'atras', modulos_path, :class => 'btn btn-primary' %>
