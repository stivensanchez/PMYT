<%- model_class = Instructor -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:tipo_doc_id) %>:</strong></dt>
  <dd><%= @instructor.tipo_doc_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:numero_doc) %>:</strong></dt>
  <dd><%= @instructor.numero_doc %></dd>
  <dt><strong><%= model_class.human_attribute_name(:nombres) %>:</strong></dt>
  <dd><%= @instructor.nombres %></dd>
  <dt><strong><%= model_class.human_attribute_name(:apellidos) %>:</strong></dt>
  <dd><%= @instructor.apellidos %></dd>
  <dt><strong><%= model_class.human_attribute_name(:correo) %>:</strong></dt>
  <dd><%= @instructor.correo %></dd>
  <dt><strong><%= model_class.human_attribute_name(:telefono) %>:</strong></dt>
  <dd><%= @instructor.telefono %></dd>
  <dt><strong><%= model_class.human_attribute_name(:modulo_id) %>:</strong></dt>
  <dd><%= @instructor.modulo_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              instructor_path(instructor), :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_instructor_path(instructor), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              instructor_path(@instructor),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
