<%- model_class = Nivelation -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:sam) %>:</strong></dt>
  <dd><%= @nivelation.sam %></dd>
  <dt><strong><%= model_class.human_attribute_name(:tiempo_real) %>:</strong></dt>
  <dd><%= @nivelation.tiempo_real %></dd>
  <dt><strong><%= model_class.human_attribute_name(:desempeño) %>:</strong></dt>
  <dd><%= @nivelation.desempeño %></dd>
  <dt><strong><%= model_class.human_attribute_name(:comentario) %>:</strong></dt>
  <dd><%= @nivelation.comentario %></dd>
  <dt><strong><%= model_class.human_attribute_name(:oper_id) %>:</strong></dt>
  <dd><%= @nivelation.oper_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:development_id) %>:</strong></dt>
  <dd><%= @nivelation.development_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ensamble_id) %>:</strong></dt>
  <dd><%= @nivelation.ensamble_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:termination_id) %>:</strong></dt>
  <dd><%= @nivelation.termination_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              nivelations_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_nivelation_path(@nivelation), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              nivelation_path(@nivelation),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
