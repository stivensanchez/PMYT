<%- model_class = TipoDoc -%>
<div class="well">
  <h1><center>Tipo <small>Documento</small></center></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:nombre) %>:</strong></dt>
  <dd><%= @tipo_doc.nombre %></dd>
  <dt><strong><%= model_class.human_attribute_name(:descripcion) %>:</strong></dt>
  <dd><%= @tipo_doc.descripcion %></dd>
</dl>

<%= link_to 'Atras', 
              tipo_docs_path, :class => 'btn btn-default'  %>
