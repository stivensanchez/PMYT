<%- model_class = Ensamble -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:operation_id) %>:</strong></dt>
  <dd><%= @ensamble.operation_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:xamp) %>:</strong></dt>
  <dd><%= @ensamble.xamp %></dd>
  <dt><strong><%= model_class.human_attribute_name(:maquina) %>:</strong></dt>
  <dd><%= @ensamble.maquina %></dd>
  <dt><strong><%= model_class.human_attribute_name(:calibre_aguja) %>:</strong></dt>
  <dd><%= @ensamble.calibre_aguja %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ppp) %>:</strong></dt>
  <dd><%= @ensamble.ppp %></dd>
  <dt><strong><%= model_class.human_attribute_name(:margen_costura) %>:</strong></dt>
  <dd><%= @ensamble.margen_costura %></dd>
  <dt><strong><%= model_class.human_attribute_name(:guia_accesorios_string) %>:</strong></dt>
  <dd><%= @ensamble.guia_accesorios_string %></dd>
  <dt><strong><%= model_class.human_attribute_name(:observaciones) %>:</strong></dt>
  <dd><%= @ensamble.observaciones %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              ensambles_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_ensamble_path(@ensamble), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              ensamble_path(@ensamble),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
