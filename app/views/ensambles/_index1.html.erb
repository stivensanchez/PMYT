<%- model_class = Ensamble -%>
  <div class="page-header">
    <h1><center> Ensamble </center></h1>
</div>
      <div class="well">
          <%= link_to t('.new', :default => t("helpers.links.new")),
            new_ensamble_path,
            :class => 'btn btn-primary ' %>
</div>
<table class="table table-bordered table-striped table-hover table-condensed">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:operation_id) %></th>
      <th><%= model_class.human_attribute_name(:sam) %></th>
      <th><%= model_class.human_attribute_name(:maquina) %></th>
      <th><%= model_class.human_attribute_name(:calibre_aguja) %></th>
      <th><%= model_class.human_attribute_name(:ppp) %></th>
      <th><%= model_class.human_attribute_name(:margen_costura) %></th>
      <th><%= model_class.human_attribute_name(:guia_accesorios) %></th>
      <th><%= model_class.human_attribute_name(:observaciones) %></th>
      <th><%= model_class.human_attribute_name(:imagen)%></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% Ensamble.all.each do |ensamble| %>
      <tr>
        <td><%= link_to ensamble.id, ensamble_path(ensamble) %></td>
        <td><%= ensamble.operation_id %></td>
        <td><%= ensamble.sam %></td>
        <td><%= ensamble.maquina %></td>
        <td><%= ensamble.calibre_aguja %></td>
        <td><%= ensamble.ppp %></td>
        <td><%= ensamble.margen_costura %></td>
        <td><%= ensamble.guia_accesorios %></td>
        <td><%= ensamble.observaciones %></td>
        <td><%= ensamble.imagen %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_ensamble_path(ensamble), :class => 'btn btn-default btn-xs btn-lg active' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      ensamble_path(ensamble),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger btn-lg active' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

